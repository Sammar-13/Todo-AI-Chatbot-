{
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next@latest"
    },
    {
      "src": "backend/api/index.py",
      "use": "@vercel/python@latest"
    }
  ],
  "routes": [
    {
      "src": "^/api(/.*)?$",
      "dest": "backend/api/index.py"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  }
}
